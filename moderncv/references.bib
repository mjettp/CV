	Doi = {10.1016/j.jaci.2015.08.045},
	Journal = {J Allergy Clin Immunol},
	Journal-Full = {The Journal of allergy and clinical immunology},
	Keywords = {(3)He magnetic resonance imaging; Hyperpolarized; childhood asthma; severe asthma; ventilation defects; ventilation heterogeneity},
	Month = {Oct},
	Pmid = {26521043},
	Pst = {aheadofprint},
	Title = {Clinical correlates of lung ventilation defects in asthmatic children},
