	Doi = {10.1080/02699052.2016.1219063},
	Journal = {Brain Inj},
	Journal-Full = {Brain injury},
	Keywords = {Neuroimaging; brain imaging; magnetic resonance imaging; phantoms; veterans; volumetrics},
	Number = {12},
	Pages = {1442-1451},
	Pmid = {27834540},
	Pst = {ppublish},
	Title = {Quantitative structural neuroimaging of mild traumatic brain injury in the {C}hronic {E}ffects of {N}eurotrauma {C}onsortium ({CENC}): Comparison of volumetric data within and across scanners},
	Volume = {30},
