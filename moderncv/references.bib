	Bdsk-Url-1 = {http://dx.doi.org/10.1097/RTI.0000000000000218}}

@article{Filiano:2016aa,
	Abstract = {Immune dysfunction is commonly associated with several neurological and mental disorders. Although the mechanisms by which peripheral immunity may influence neuronal function are largely unknown, recent findings implicate meningeal immunity influencing behaviour, such as spatial learning and memory. Here we show that meningeal immunity is also critical for social behaviour; mice deficient in adaptive immunity exhibit social deficits and hyper-connectivity of fronto-cortical brain regions. Associations between rodent transcriptomes from brain and cellular transcriptomes in response to T-cell-derived cytokines suggest a strong interaction between social behaviour and interferon-γ (IFN-γ)-driven responses. Concordantly, we demonstrate that inhibitory neurons respond to IFN-γ and increase GABAergic (γ-aminobutyric-acid) currents in projection neurons, suggesting that IFN-γ is a molecular link between meningeal immunity and neural circuits recruited for social behaviour. Meta-analysis of the transcriptomes of a range of organisms reveals that rodents, fish, and flies elevate IFN-γ/JAK-STAT-dependent gene signatures in a social context, suggesting that the IFN-γ signalling pathway could mediate a co-evolutionary link between social/aggregation behaviour and an efficient anti-pathogen response. This study implicates adaptive immune dysfunction, in particular IFN-γ, in disorders characterized by social dysfunction and suggests a co-evolutionary link between social behaviour and an anti-pathogen immune response driven by IFN-γ signalling.},
	Author = {Filiano, Anthony J and Xu, Yang and Tustison, Nicholas J and Marsh, Rachel L and Baker, Wendy and Smirnov, Igor and Overall, Christopher C and Gadani, Sachin P and Turner, Stephen D and Weng, Zhiping and Peerzade, Sayeda Najamussahar and Chen, Hao and Lee, Kevin S and Scott, Michael M and Beenhakker, Mark P and Litvak, Vladimir and Kipnis, Jonathan},
	Date-Added = {2017-06-19 18:15:13 +0000},
	Date-Modified = {2017-07-05 16:15:25 +0000},
	Doi = {10.1038/nature18626},
	Journal = {Nature},
	Journal-Full = {Nature},
	Mesh = {Animals; Drosophila melanogaster; Female; GABAergic Neurons; Interferon-gamma; Male; Meninges; Mice; Mice, Inbred C57BL; Neural Pathways; Prefrontal Cortex; Rats; Signal Transduction; Social Behavior; T-Lymphocytes; Transcriptome; Zebrafish},
	Month = {07},
